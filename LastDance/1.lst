				;=============================================================================
				; макроопределение (далее ваше решение):
				Max macro LV, r32
				    Local flag4
				    flag4 = 0

				    ifidni <r32>,<ESP>
				        exitm
				    endif
				    for p,<LV>
				        ifnb <p>
				            if type p NE 4
				                flag4 = 1
				            endif
				        endif
				    endm
				    mov r32, 0
				    if flag4 EQ 1
				        push r32
				    endif
				    
				    for p,<LV>
				        Local no
				        ifnb <p>
				            
				            if flag4 EQ 1
				                if type p NE 4
				                    movzx r32, p
				                    cmp r32, [ESP]
				                        jBE no
				                    mov [ESP], r32
				                    no:
				                else
				                    mov r32, p
				                    cmp r32,[ESP]
				                        jBE no
				                    mov [ESP], r32
				                    no:
				                endif
				            else
				                cmp r32, p
				                    jAE no
				                mov r32, p
				                no:
				            
				                    
				            endif
				        endif
				    endm
				    if flag4 EQ 1
				        pop r32
				    endif
				                    
				    
				endm


